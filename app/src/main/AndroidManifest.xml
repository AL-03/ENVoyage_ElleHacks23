<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.EnVoyage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="En Voyage"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TheGuessingGame">
        <activity
            android:name="com.example.EnVoyage.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.EnVoyage.Pop"></activity>
        <activity android:name="com.example.EnVoyage.PopTwo"></activity>
    </application>

</manifest>